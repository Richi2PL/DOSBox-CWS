{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "DOSBox",
  "short_name": "DOSBox",
  "description": "DOS emulator",
  "version": "2.7.1",
  "minimum_chrome_version": "88",
  "manifest_version": 3,
  "offline_enabled": true,
  "permissions": [
    "system.cpu",
    "storage",
    "unlimitedStorage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/dosbox-16.png"
    }
  },
  "icons": { 
    "16": "icons/dosbox-16.png",
    "22": "icons/dosbox-22.png",
    "24": "icons/dosbox-24.png",
    "32": "icons/dosbox-32.png",
    "36": "icons/dosbox-36.png",
    "48": "icons/dosbox-48.png",
    "64": "icons/dosbox-64.png",
    "72": "icons/dosbox-72.png",
    "96": "icons/dosbox-96.png",
    "128": "icons/dosbox-128.png",
    "192": "icons/dosbox-192.png"
  },
  "platforms": [
    {
      "nacl_arch": "x86-64",
      "sub_package_path": "_platform_specific/x86-64/"
    },
    {
      "nacl_arch": "x86-32",
      "sub_package_path": "_platform_specific/x86-32/"
    },
    {
      "nacl_arch": "arm",
      "sub_package_path": "_platform_specific/arm/"
    }
  ]
}
